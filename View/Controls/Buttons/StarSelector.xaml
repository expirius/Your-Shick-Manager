<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MFASeeker.View.Controls.Buttons.StarSelector"
             x:Name="StarSelectorName">
    <HorizontalStackLayout x:Name="StarSelectorLayout" HorizontalOptions="CenterAndExpand">
        <ImageButton x:Name="Star1" HeightRequest="60" WidthRequest="60" Command="{Binding StarSelectedCommand}" CommandParameter="1"/>
        <ImageButton x:Name="Star2" HeightRequest="60" WidthRequest="60" Command="{Binding StarSelectedCommand}" CommandParameter="2"/>
        <ImageButton x:Name="Star3" HeightRequest="60" WidthRequest="60" Command="{Binding StarSelectedCommand}" CommandParameter="3"/>
        <ImageButton x:Name="Star4" HeightRequest="60" WidthRequest="60" Command="{Binding StarSelectedCommand}" CommandParameter="4"/>
        <ImageButton x:Name="Star5" HeightRequest="60" WidthRequest="60" Command="{Binding StarSelectedCommand}" CommandParameter="5"/>
    </HorizontalStackLayout>
</ContentView>
